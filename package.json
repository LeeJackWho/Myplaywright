{
  "name": "playwright-automation",
  "version": "1.0.0",
  "description": "Playwright automation framework",
  "scripts": {
    "cleanup": "ts-node utils/cleanup.ts",
    "test": "npm run cleanup && playwright test",
    "test:headed": "npm run cleanup && playwright test --headed",
    "test:ui": "npm run cleanup && playwright test --ui",
    "test:debug": "npm run cleanup && playwright test --headed --debug"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1"
  }
}